/* pages/agreement/user/user.wxss */

.userContainer {
  height: 100%;
}

.userContainer .header {
  width: 100%;
  height: 140rpx;
  background-color: #9cc0fe;
  padding-top: 10rpx;
}

.userContainer .header text {
  display: flex;
  flex-direction: column;
  align-items: center;
  color: white;
  font-size: 30rpx;
}
.userContainer .body {
  height: 100%;
  margin: -13% 0 0;
  padding: 30rpx 20rpx;
  background-color: #fff;
}

.title {
  font-size: 40rpx;
  font-weight: bold;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-bottom: 30rpx;
}

.content {
  font-size: 28rpx;
  color: #666666;
}

.app {
  font-size: 30rpx;
  color: #666666;
  font-weight: bold;
}

.contentTitle {
  margin-top: 30rpx;
  font-size: 35rpx;
  color: #333;
  font-weight: bold;
}

.contents {
  margin-top: 10rpx;
}